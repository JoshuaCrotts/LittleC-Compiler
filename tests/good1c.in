
// Similar to good1b, but using a for loop

int pow(int a, int b) {
    int i;
    int res = 1;
    for (i=0; i<b; i++) {
        res = res * a;
    }
    return res;
}

void main() {
    int i;
    int res = 1;
    int b = 6;
    int a = 2;
    for (i=0; i<b; i++) {
        res = res * a;
    }
    
    printd(res);
}

        
